{
  "name": "form-pdf-ui",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./dist ./build",
    "dev": "vite --mode development",
    "build:fe": "vite build --mode production --outDir ./build/dist",
    "build": "bun run clean && bun run build:fe",
    "preview": "vite preview --mode production --outDir ./build/dist --host 0.0.0.0 --port 35500",
    "start": "bun run build && bun run preview",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@codexteam/icons": "^0.3.3",
    "@editorjs/checklist": "^1.6.0",
    "@editorjs/code": "^2.9.3",
    "@editorjs/delimiter": "^1.4.2",
    "@editorjs/editorjs": "^2.30.8",
    "@editorjs/embed": "^2.7.6",
    "@editorjs/header": "^2.8.8",
    "@editorjs/image": "^2.10.2",
    "@editorjs/inline-code": "^1.5.1",
    "@editorjs/link": "^2.6.2",
    "@editorjs/list": "^2.0.8",
    "@editorjs/marker": "^1.4.0",
    "@editorjs/paragraph": "^2.11.7",
    "@editorjs/quote": "^2.7.6",
    "@editorjs/table": "^2.4.4",
    "@editorjs/underline": "^1.2.1",
    "@editorjs/warning": "^1.4.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-pdf-viewer/page-navigation": "^3.12.0",
    "@tailwindcss/vite": "^4.1.11",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i": "^0.3.7",
    "lodash": "^4.17.21",
    "lucide-react": "^0.535.0",
    "npm": "^11.4.1",
    "pdf-lib": "^1.17.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "signature_pad": "^5.0.9",
    "tailwind-merge": "^3.3.1",
