    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/uuid": "^10.0.0",
    "pdfjs-dist": "3.4.120",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
